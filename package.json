{
  "name": "gooditworks_base_frontend",
  "version": "0.2.3",
  "description": "Gooditworks base frontend project",
  "repository": "https://github.com/gooditworks/base-frontend",
  "author": "Max Eliseev <mxseev@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && next start",
    "postinstall": "sh ./scripts/add_base_remote.sh",
    "lint": "concurrently -c green \"npm:lint:*\"",
    "lint:base": "sh ./scripts/check_base_update.sh",
    "lint:typecheck": "tsc --noEmit --project tsconfig.json",
    "lint:next": "next lint",
    "test": "concurrently -c green \"npm:test:*\"",
    "test:unit": "jest",
    "test:e2e": "npm run playwright:test",
    "playwright:test": "playwright test",
    "playwright:install": "playwright install",
    "playwright:inspect": "PWDEBUG=1 playwright test",
    "playwright:update-snapshots": "playwright test --update-snapshots",
    "release": "standard-version",
    "commit": "git add . && cz"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.7",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@playwright/test": "1.22.2",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.5",
    "@types/react": "18.0.12",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "autoprefixer": "10.4.7",
    "commitizen": "4.2.4",
    "commitlint-plugin-function-rules": "1.7.1",
    "concurrently": "7.2.1",
    "cssnano": "5.1.11",
    "cz-conventional-changelog": "3.3.0",
    "enhanced-resolve": "5.9.3",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-jest": "26.5.3",
    "jest": "28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-junit": "13.2.0",
    "postcss": "8.4.14",
    "postcss-flexbugs-fixes": "5.0.2",
    "prettier": "2.6.2",
    "standard-version": "9.5.0",
    "tailwindcss": "3.0.24",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@gooditworks/monitoring": "^1.3.0",
    "classnames": "2.3.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  }
}
